<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Ticket Difficulties</title>
</head>
<body>
  <div id="ticketData"></div>
  
  <script>
    // Fetch data from backend
    fetch('http://localhost:3000/data')
      .then(response => response.json())
      .then(data => {
        const ticketDataElement = document.getElementById('ticketData');
        // Display data
        data.forEach(ticket => {
          ticketDataElement.innerHTML += `<p>${ticket.columnName}</p>`; // Replace "columnName" with the actual column name
        });
      })
      .catch(error => console.error('Error fetching data:', error));
  </script>
</body>
</html>

